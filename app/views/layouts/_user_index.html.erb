
<div>
  <h1>My Details</h1>
  <div class="row">
    <div class="col-md-6">
      <table class="table table-striped">
        <tbody>
          <tr><td>Name</td><td><%= current_user.name %></td></tr>
          <tr><td>Address</td><td><%= current_user.address %></td></tr>
          <tr><td>Email</td><td><%= current_user.email %></td></tr>
        </tbody>
        <tfoot>
          <tr>
            <td><%= link_to 'Update Details', edit_user_registration_path %></td>
          </tr>
        </tfoot>
      </table>
    </div>
  </div>
</div>

<div>
  <h1><%= link_to 'View My Dogs', user_dogs_path(user_id: current_user.id) %></h1>
  <h1><%= link_to 'Register New Dog', new_user_dog_path(user_id: current_user.id) %></h1>
</div>




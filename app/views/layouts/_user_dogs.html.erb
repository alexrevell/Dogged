<div>
  <h1>My Dogs</h1>

  <p><%= link_to 'Register New Dog', new_user_dog_path(current_user) %></p>

  <% current_user.dogs.each do |dog| %>
  <div class="row">
    <div class="col-md-6">
      <table class="table table-striped">

        <tbody>
          <tr><td>Name</td><td><%= dog.name %></td></tr>
          <tr><td>Breed</td><td><%= dog.breed %></td></tr>
          <tr><td>DOB</td><td><%= dog.date_of_birth %></td></tr>
          <tr><td>ID</td><td><%= dog.id %></td></tr>
          <tr><td>Registered Until</td><td><%= dog.registered_until %></td></tr>
          <tr><td><%= link_to 'View Dog', user_dog_path(user_id: current_user.id, id: dog.id) %></td></tr>
        </tbody>
      </table>
    </div>
  </div>
  <% end %>
</div>
